public Map<String, Decimal> industrySummary(List<Account> accounts) {
    Map<String, Decimal> returnMap = new Map<String, Decimal>();

    for (Account a : accounts) {
        if (!returnMap.containsKey(a.Name)) {
            returnMap.put(a.name, a.AnnualRevenue);
        }
        else {
            Decimal currentSum = returnMap.get(a.name);
            Decmual updatedSum = currentSum + a.AnnualRevenue;

            returnMap.put(a.Name, updatedSum):
        }
    }

    return returnMap;

}