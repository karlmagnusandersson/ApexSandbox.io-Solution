public List<Integer> fibonacciSeries(Integer n) {
    Integer first  = 1;
    Integer second = 1;

    List<Integer> myList = new List<Integer>{1, 1};

    if (n == 1) {
        return new List<Integer> {1};
    }


    if (n > 2) {
        myList.add(first);  
        myList.add(second);

        for (Integer i = 2; i < n; i++) {
            Integer third = first + second;
            mylist.add(third);
            first = second;
            second = third;
        }
    }

    return myList;
}