public List<Integer> fibonacciSeries(Integer n) {
    Integer first  = 1;
    Integer second = 1;

    List<Integer> myList = new List<Integer>{first, second};

    for (Integer i = 0; i < n; i++) {
        Integer third = first + second;
        mylist.add(third);
        first = second;
        second = third;
    }

    return myList;
}